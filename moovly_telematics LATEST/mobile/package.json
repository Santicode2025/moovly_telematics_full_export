{
  "name": "moovly-driver-app",
  "version": "1.0.0",
  "description": "Moovly Fleet Management - Driver Mobile App",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:apk": "eas build --platform android --profile apk",
    "build:ipa": "eas build --platform ios --profile apk",
    "build:both": "eas build --platform all --profile apk",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "update": "expo update"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native-stack": "^6.9.17",
    "@tanstack/react-query": "^5.17.15",
    "expo": "~50.0.6",
    "expo-camera": "~14.0.5",
    "expo-location": "~16.5.5",
    "expo-media-library": "~15.9.1",
    "expo-notifications": "~0.27.6",
    "expo-secure-store": "~12.8.1",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-async-storage": "^1.21.0",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-maps": "1.10.0",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-vector-icons": "^10.0.3",
    "axios": "^1.6.5",
    "date-fns": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.72.8",
    "typescript": "^5.1.3"
  },
  "private": true
